{
	"name": "@yiird/elements",
	"version": "0.1.0-alpha.1",
	"private": false,
	"main": "dist/yiird-elements.umd.js",
	"module": "dist/yiird-elements.mjs",
	"types": "dist/index.d.ts",
	"license": "ISC",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/yiird-elements.mjs",
			"require": "./dist/yiird-elements.umd.js"
		},
		"./button": {
			"import": "./dist/standalone/button.mjs",
			"require": "./dist/standalone/button.umd.js"
		},
		"./icon": {
			"import": "./dist/standalone/icon.mjs",
			"require": "./dist/standalone/icon.umd.js"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only && bash scripts/build-standalone.sh",
		"build:main": "run-p type-check build-only",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"format": "prettier --write src/",
		"commit": "git add . && cz"
	},
	"dependencies": {
		"vue": "^3.2.47"
	},
	"devDependencies": {
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.14.2",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vitejs/plugin-vue-jsx": "^3.0.0",
		"@vue/test-utils": "^2.3.0",
		"@vue/tsconfig": "^0.1.3",
		"@yiird/handler": "^0.1.1",
		"jsdom": "^21.1.0",
		"npm-run-all": "^4.1.5",
		"vite": "^4.1.4",
		"vite-plugin-dts": "^2.2.0",
		"vitest": "^0.29.1",
		"vue-tsc": "^1.2.0"
	},
	"gitHead": "cef8d4b4cfa6c01769a1a810baa69f2bd451c461"
}
