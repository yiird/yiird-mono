{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts","src/**/*.ts", "src/**/*.vue", "packages/**/*.ts", "packages/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"ignoreDeprecations": "5.0",
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
