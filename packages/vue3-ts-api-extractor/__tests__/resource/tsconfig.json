{
  "compilerOptions": {
    "target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"jsx": "preserve",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"allowJs": false,
		"baseUrl":".",
		"skipLibCheck":true,
		"lib": ["esnext","DOM"],
  },
  "include": ["**/*.ts", "**/*.d.ts", "**/*.tsx", "**/*.vue"],
  "exclude": ["/node_modules/"]
}
